#include <iostream>
void Sapxep(int a[], int n)
{
    for (int = 0; i < n; i++)
        for(int j = i + 1; j < n; j++)
        if(a[i]>a[j])
            swap(a[i], a[j]);
}

void swap(int &a, int &b)
{
    int x = a;
    a = b;
    b = x;
}

void NhapMang(int a[], int &n)
{
    do{
        printf("Cho biet so phan tu cua mang: ");
        scanf("%d", &n);
    }while (n<=0);
    for(int i=0; i<n; i++)
    {
        printf("Gia tri phan tu a[%d]=",i);
        scanf("%d", &a[i]);
    }
}

void main()
{
    int A[100];
    NhapMang(A, N);
    Sapxep(A, N);
    XuatMang(A,N);
    getch();
}